provider_installation {
    network_mirror {
        url = "https://terraform-mirror.yandexcloud.net/"
        include = ["registry.terraform.io/*/*"]
        exclude = ["registry.terraform.io/ovirt/ovirt"]
    }
    filesystem_mirror {
        path    = "/home/gitlab-runner/.terraform.d/plugins/"
        include = ["registry.terraform.io/ovirt/ovirt"]
    }
    direct {
        exclude = ["registry.terraform.io/*/*"]
    }
}